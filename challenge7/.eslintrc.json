{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "import/no-unresolved": "off",
        "camelcase": "off",
        "no-console": "off"
    }
}
